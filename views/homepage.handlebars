<!-- Loop over each object in the projects array -->
{{#each posts as |post| }}
<div class="w-80">
  <a class=" links post-container" href="/api/posts/{{post.id}}">
    <p class="fs-2 tt-c">{{post.title}}</p>
    
    <p>Created by {{post.user.username}} on {{format_date post.date_created}}</p>
  </a>
  
</div>
{{/each}}
